{
  "name": "egg-react-webpack-mpa-boilerplate",
  "version": "2.2.0",
  "description": "基于 easywebpack-react 和 egg-view-react-ssr 插件服务端渲染工程骨架项目",
  "scripts": {
    "build": "easy build",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "clean": "easy clean",
    "start": "egg-scripts start --port 7001 --workers 4",
    "backend": "nohup egg-scripts start --port 7001 --workers 4 &",
    "kill-port": "kill -9 $(lsof -i:8888 |grep 'node'|aw '{print $2}') ",
